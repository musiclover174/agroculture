html,
body
  font-family $os
body
  overflow-x: hidden
  &.inner
    pos relative
    bgc $dark
    .content
      pt 190px
    .container
      @media $lDesktopMedium
        max-width 100%
        padding 0 30px
    .header
      pos absolute
    &:before
      content ""
      position fixed
      l 0
      t 0
      w 100vw
      h 100vh
      z-index -1
      opacity .5
      background url('../i/inner-bg.jpg') no-repeat center
      background-size cover
  
.container
  w 100%
  max-width 1600px
  margin 0 auto
  @media $lDesktopMax
    max-width 1400px
  @media $lDesktopMedium
    max-width 1250px
  @media $lDesktopSmall
    max-width 1136px
  @media $desktopMax
    max-width 100%
    padding 0 30px
  @media $LandscapeMedium
    padding 0 20px
    
.content
  h1:not([class])
    color white
    max-width 830px
    fontify 72px 400 76px
    margin 63px 0 35px
    @media $lDesktopMedium
      fontify 60px 400 64px
      max-width 720px
    @media $desktopMin
      fontify 48px 400 52px
    @media $LandscapeMedium
      fontify 36px 400 42px
  h2:not([class])
    color $dark
    fontify 42px 400 46px
    margin 63px 0 28px
    @media $desktopMax
      fontify 38px 400 42px
    @media $lDesktopSmall
      fontify 36px 400 40px
    @media $LandscapeMedium
      fontify 30px 400 34px
      margin 45px 0 28px
  h3:not([class])
    color $dark
    fontify 30px 400 36px
    margin 65px 0 38px
    @media $LandscapeMedium
      fontify 26px 400 34px
      margin 45px 0 28px
  h4:not([class])
    color $dark
    fontify 24px 400 34px
    margin 43px 0 16px
  img:not([class])
    margin 5px 0
    max-width 100%
  ul:not([class])
    list-style  none
    margin  23px 0
  ul:not([class]) li
    pl 30px
    pos relative
    fontify 20px 400 27px
    @media $lDesktopSmall
      fontify 17px 400 24px
  ul:not([class]) li + li
    mt 25px
  ul:not([class]) li:before
    content ""
    pos absolute
    l 0
    t 13px
    size 14px 1px
    bgc $dark
  ol:not([class])
    list-style none
    margin 23px 0
    counter-reset myCounter
  ol:not([class]) li
    pl 30px
    pos relative
    fontify 20px 400 27px
    @media $lDesktopSmall
      fontify 17px 400 24px
  ol:not([class]) li + li
    mt 24px
  ol:not([class]) li:before
    counter-increment myCounter
    content counter(myCounter)"."	
    pos absolute
    l 0
    t 0
    w 30px
    pr 9px
    color $orange
    fontify 20px 400 27px
  p:not([class])
    color $dark
    fontify 20px 400 27px
    margin 23px 0
    @media $lDesktopSmall
      fontify 17px 400 24px
  a:not([class])
    color $orange
    border-bottom 1px solid
    transition .5s border
    &:hover
      border-color transparent
 
  *:not([class]):not(span):first-child
    mt 0 !important
  *:not([class]):not(span):last-child
    margin-bottom 0 !important

  table
    margin 30px 0
    border 2px solid #9db038
  table th
    text-align left
    padding 16px 24px
    color $dark
    bgc $orange
    fontify 15px 400 21px
    border 1px solid #f0f3e1
  table td
    padding 18px 24px
    color $dark
    fontify 15px 400 21px
    border 1px solid #f0f3e1

input::-webkit-input-placeholder
  color alpha(white, .5)
input::-moz-placeholder
  color alpha(white, .5)
input:-ms-input-placeholder
  color alpha(white, .5)

div[class\^="table-scroller"]
  overflow auto
  pos relative
  @media $desktopMax
    w 100%

.btn
  ib()
  bgc white
  border 0
  border-radius 18px
  color $dark
  outline none
  cursor pointer
  fontify 14px 700 36px
  letter-spacing .1em
  text-transform uppercase;
  padding 0 20px
  transition .7s background, .7s color, .7s box-shadow
  @media $lDesktopMedium
    fontify 12px 700 34px
  &:hover
    bgc $green
    color white
    box-shadow 0 0 15px 0 alpha($green, 0.8)
    
.common
  &__title
    color $dark
    fontify 44px 400 54px
    mb 25px
  &__back
    ib()
    pos relative
    mt 75px
    ml 56px
    color $dark
    fontify 23px 400 21px
    border-bottom 1px solid alpha($dark, .25)
    transition .5s border
    @media $LandscapeMedium
      fontify 17px 400 21px
      ml 41px
    &:hover
      border-color transparent
    &:before
      content ""
      pos absolute
      l -56px
      t -1px
      size 27px 25px
      background url("data:image/png;base64,") no-repeat center
      @media $LandscapeMedium
        l -41px
  &__input
    w 100%
    h 48px
    padding 0 18px
    border 2px solid #ececec
    bgc transparent
    color $dark
    font-family $os
    font-size 14px
    line-height 400
    transition .5s border
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      dn()
  &__textarea
    w 100%
    h 116px
    overflow hidden
    resize none
    padding 11px 18px
    border 2px solid #ececec
    bgc transparent
    color $dark
    font-family $os
    fontify 14px 400 24px
    transition .5s border
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      dn()
  &__sbm
    padding 0 42px
    font-family FiraSans
  &__select
    h 36px
    bgc #fff
    border 2px solid #b6b6b6
    w auto
    cursor pointer
    transition .5s border
    &.warning
      border-color #ed1c24
  &__placeholder
    pos absolute
    z-index -1
    l 0 
    t 0
    pl 20px
    color alpha($dark, .5)
    fontify 14px 400 50px
    text-overflow ellipsis
    white-space nowrap
    overflow hidden
    max-width 100%
    span
      color $orange
  &__col
    w calc(50% - 14px)
    &_iroad
      max-width 510px
      @media $LandscapeMedium
        w 100%
        &:first-child
          dn()
  &__row
    display flex
    justify-content space-between
    pos relative
    w 100%
    & + &
      mt 20px
  &__fileover
    pos relative
    & + &
      mt 20px
  &__file
    pos absolute
    w 0
    h 0
    l 0
    t 0
    opacity 0
    &name
      ib()
      pos relative
      l 26px
      cursor pointer
      color $dark
      fontify 14px 400 18px
      border-bottom 2px dotted $orange
      transition .5s border
      &.choosed,
      &:hover
        border-color transparent
      &:before
        content ""
        pos absolute
        l -24px
        t 6px
        size 16px 8px
        background url("data:image/png;base64,") no-repeat center
      &.choosed + ^[-1]delete
        visibility visible
        cursor pointer
    &delete
      ib()
      visibility hidden
      mt 5px
      ml 32px
      size 9px
      background url("data:image/png;base64,") no-repeat center
      
.page
  &__top
    mb 65px
    padding 0 85px
    &_pzero
      padding 0
    @media $desktopMax
      padding 0 50px
    @media $LandscapeMedium
      padding 0
      mb 45px
  &__withside
    display flex
    flex-direction row-reverse
    align-items flex-start
    justify-content space-between
    @media $LandscapeMedium
      db()
  &__content
    bgc white
    padding 74px 85px 24px
    @media $desktopMax
      padding 40px 50px 24px
    @media $LandscapeMedium
      padding 40px 30px 24px
      ml -30px
      w calc(100% + 60px)
  &__aside
    w 430px
    @media $lDesktopMedium
      w 385px
    @media $desktopMedium
      w 315px
    @media $desktopMin
      dn()
    &-sticky
      padding 0 20px 0 80px
      @media $lDesktopMedium
        padding 0 20px 0 50px
  &__aside + &__content
    w calc(100% - 430px)
    @media $lDesktopMedium
      w calc(100% - 385px)
    @media $desktopMedium
      w calc(100% - 315px)
    @media $desktopMin
      w 100%
    @media $LandscapeMedium
      w calc(100% + 60px)

.fakecheckbox
  ib()
  pos relative
  &__label
    ib()
    color $dark
    fontify 14px 400 24px
    pos relative
    pl 37px
    pt 7px
    &:before 
      content ""
      pos absolute
      l 0px
      t 7px
      size 20px
      border 2px solid #ececec
      cursor pointer
      transition .5s border
    &:after
      content ""
      pos absolute
      size 16px 13px
      background url("data:image/png;base64,") no-repeat center
      l 4px
      t 13px
      opacity 0
      cursor pointer
      transition .5s opacity
  &__truecheck
    pos absolute
    l 0
    t 0
    w 0
    h 0
    border 0
    opacity 0
    &:checked + ^[0]__label:after
      opacity 1
    &.warning + ^[0]__label:before 
      border-color #ed1c24
    
.totop
  pos fixed
  z-index 5
  b 20px
  r 20px
  cursor pointer
  size 44px
  border-radius 50%
  box-shadow 0 6px 19px rgba(0, 0, 0, 0.12)
  visibility hidden
  opacity 0
  background url("data:image/png;base64,") no-repeat center #e76f17
  transition .5s visible, .5s box-shadow, .5s opacity
  &:hover
    box-shadow none
  &.visible
    visibility visible
    opacity 1
      
.breadcrumbs
  mb 18px
  color white
  pl 106px
  pos relative
  @media $LandscapeMedium
    mt -20px
    pl 0
  &:before
    content ""
    pos absolute
    l 6px
    t 11px
    w 70px
    h 1px
    bgc white
    @media $LandscapeMedium
      dn()
  &__href
    color white
    fontify 14px 400 18px
    border-bottom 1px solid white
    transition .5s border
    &:hover
      border-color transparent
  &__delim
    padding 0 11px
    fontify 12px 400 18px

input.warning,
textarea.warning
  border-color #ff3a0f

.hidden-block
  display none
  
.tippy-tooltip.light-theme
  bgc white
  box-shadow 0 6px 32px rgba(0, 0, 0, 0.21)
  fontif 14px 400 18px
  color $dark
  padding 15px 19px
  .tippy-backdrop
    bgc white
    color $dark
  .tippy-arrow
    border-top-color white
    
@keyframes arrAnimation {
  0% { background-position: 0 0; }
  100% { background-position: 0 -756px; }
}